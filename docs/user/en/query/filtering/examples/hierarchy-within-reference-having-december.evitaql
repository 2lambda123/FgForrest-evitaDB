query(
    collection('Product'),
    filterBy(
        hierarchyWithin(
            'categories',
            attributeEquals('code', 'accessories'),
            having(
                attributeInRange('validity', '2023-12-02T01:00:00-01:00')
            )
        )
    ),
    require(
        entityFetch(
            attributes('code')
        )
    )
)